	/**
	 * Draw the UI
	 */
	function drawUI() {
		document.getElementById('main-ui').classList.add('main-ui-anim');
		document.getElementById('main-ui-bar').classList.add('main-ui-bar-anim');

		document.getElementById('main-ui-close').addEventListener('click', function () {
			blueScreen();
		});
	}

	/**
	 * Loading process
	 */
	let loadingLi = document.querySelectorAll('#loading-list li');
	function showLI(i = 0) {
		if (loadingLi[i] === undefined) { drawUI(); return; }
		loadingLi[i].style.display = "block";

		setTimeout(function () { showLI(i + 1); }, 100 + Math.round(Math.random() * (400 - i * 50)));
	}
	showLI();
</script>